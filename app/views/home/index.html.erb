<%= content_for :vendor_scripts do %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=false" %>
<%end%>

<%= content_for :stylesheets  do%>
  <style>
    body{
      width: <%= @width %>px;
      height: <%= @height %>px;
    }
  </style>
<%end%>
<%= content_for :scripts do %>
  <script>
    Wayfarer.mode = "<%= @mode %>";
    Wayfarer.item_icon = "<%= image_path 'item.png' %>";
    Wayfarer.comment_icon = "<%= image_path 'comment.png' %>";
    Wayfarer.dimensions = {
       width: <%= @width %>
      ,height: <%= @height %>
    };
  </script>
<%end%>

<%= render 'home/jst/comment' %>

<div id="map">
</div>

<ul id="feed-list"></div>

<div id="gallery"></div>
